    "cors": "^2.8.5",
    "mongoose": "^7.0.0",
    "multer": "^1.4.5-lts.1",
    "axios": "^1.3.4",
    "express-rate-limit": "^6.7.0",
    "crypto": "^1.0.1"
  },
  "devDependencies": {
    "concurrently": "^7.6.0",
    "nodemon": "^2.0.20"
  }
}